  <div class="row">
    <div class="col-xl-4">
      <div class="boxes">
        <h6 class="boxes__title">{{dashboard.graphs.yearly}}</h6>
        <canvas id="myChart"></canvas>

      </div>
    </div>
    <div class="col-xl-4">
      <div class="boxes">
        <h6 class="boxes__title">{{dashboard.graphs.yearly-revenue}}</h6>
