<div class="container-fluid">
  <div class="row">
    <div class="col-lg-6 col-xl-3">
      <div class="boxes boxes--right">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--success">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#components"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>64,570</h5>
            <p>{{dashboard.stats.visits}}</p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-xl-3">
      <div class="boxes boxes--right">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--primary">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#components"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>64,570</h5>
            <p>{{dashboard.stats.revenue}}</p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-xl-3">
      <div class="boxes boxes--right">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--danger">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#components"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>64,570</h5>
            <p>{{dashboard.stats.sales}}</p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-xl-3">
      <div class="boxes boxes--right">
        <div class="row">
          <div class="col-4">
            <div class="circle circle--border circle--secondary">
              <svg class="svg-icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#components"></use>
              </svg>
            </div>
          </div>
          <div class="col-8 v-align">
            <h5>64,570</h5>
            <p>{{dashboard.stats.conversion}}</p>
          </div>
        </div>
      </div>
    </div>
  </div>



